{
    "name": "Alex Extension!",
    "version": "1.0",
    "description": "Building a Chrome Extension with Angular",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "webNavigation",
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js",
            "runtime.js"
        ]
    },
    "browser_action": {
        "default_popup": "index.html"
    }
}